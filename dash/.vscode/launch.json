{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Example",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}/example",
      "console": "terminal",
      "vmAdditionalArgs": [
        "--enable-vm-service",
        "--observe"
      ]
    },
    {
      "name": "Example (Production)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}/example",
      "console": "terminal",
      "env": {
        "DASH_ENV": "production"
      }
    }
  ]
}